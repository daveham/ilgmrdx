{
  "name": "inferencelens-graphics-manager-redux",
  "version": "0.1.0",
  "description": "Redux-based web app for managing graphics used in InferenceLens",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf dist",
    "compile": "node -r dotenv/config --harmony bin/compile",
    "lint": "eslint . ./",
    "lint:fix": "npm run lint -- --fix",
    "start": "better-npm-run start",
    "dev": "better-npm-run dev",
    "dev:nw": "npm run dev -- --nw",
    "dev:no-debug": "npm run dev -- --no_debug",
    "test": "better-npm-run test",
    "test:dev": "npm run test -- --watch",
    "deploy": "better-npm-run deploy",
    "flow:check": "node bin/flow-check"
  },
  "betterScripts": {
    "dev": {
      "command": "nodemon -r dotenv/config bin/register",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "deploy": {
      "command": "npm run clean && npm run compile",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "start": {
      "command": "node -r dotenv/config bin/register"
    },
    "test": {
      "command": "node -r dotenv/config ./node_modules/karma/bin/karma start bin/karma.js",
      "env": {
        "NODE_ENV": "test"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/daveham/ilgmrdx.git"
  },
  "author": "Dave Hamilton",
  "license": "MIT",
  "dependencies": {
    "better-npm-run": "0.0.5",
    "body-parser": "^1.14.2",
    "connect-history-api-fallback": "^1.1.0",
    "debug": "^2.2.0",
    "dotenv": "^2.0.0",
    "express": "^4.13.3",
    "glob": "^6.0.4",
    "gm": "^1.21.1",
    "history": "^2.0.0-rc2",
    "isomorphic-fetch": "^2.2.0",
    "morgan": "^1.6.1",
    "normalize.css": "^3.0.3",
    "react": "^0.14.0",
    "react-addons-css-transition-group": "^0.14.7",
    "react-blocks": "^1.1.4",
    "react-dom": "^0.14.0",
    "react-redux": "^4.0.0",
    "react-router": "^2.0.0-rc5",
    "react-router-redux": "^3.0.0",
    "react-toolbox": "^0.14.1",
    "redux": "^3.0.0",
    "redux-actions": "^0.9.0",
    "redux-thunk": "^1.0.0",
    "reselect": "^2.0.1",
    "rimraf": "^2.5.0",
    "url": "^0.11.0",
    "yargs": "^3.18.0"
  },
  "devDependencies": {
    "babel-core": "^6.3.17",
    "babel-eslint": "^5.0.0-beta6",
    "babel-loader": "^6.2.0",
    "babel-plugin-add-module-exports": "^0.1.1",
    "babel-plugin-resolver": "0.0.7",
    "babel-plugin-transform-runtime": "^6.3.13",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-react-hmre": "^1.0.0",
    "babel-preset-stage-0": "^6.3.13",
    "babel-register": "^6.3.13",
    "babel-runtime": "^6.3.19",
    "chai": "^3.4.1",
    "chai-as-promised": "^5.1.0",
    "chai-enzyme": "^0.3.0",
    "cheerio": "^0.19.0",
    "css-loader": "^0.23.0",
    "cssnano": "^3.3.2",
    "enzyme": "^1.5.0",
    "eslint": "^1.10.3",
    "eslint-config-standard": "^4.4.0",
    "eslint-config-standard-react": "^1.2.1",
    "eslint-loader": "^1.1.1",
    "eslint-plugin-babel": "^3.0.0",
    "eslint-plugin-flow-vars": "^0.1.3",
    "eslint-plugin-react": "^3.14.0",
    "eslint-plugin-standard": "^1.3.1",
    "extract-text-webpack-plugin": "^1.0.0",
    "file-loader": "^0.8.4",
